&lt;H3 style=&quot;MARGIN: 0cm 0cm 0pt 58.35pt&quot;>&lt;a name=&quot;_Toc217213540&quot;>&lt;SPAN lang=EN-US style=&quot;mso-fareast-font-family: Arial; mso-bidi-font-family: Arial&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;FONT size=3>2.1.3&lt;/FONT>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN lang=EN-US>&lt;FONT size=3>Architecture-Centric&lt;/FONT>&lt;/SPAN>&lt;/a>&lt;/H3>&#xD;
&lt;P class=MsoBodyTextIndent style=&quot;MARGIN: 0cm 0cm 9pt 40pt; TEXT-ALIGN: center&quot; align=center>&lt;SPAN lang=EN-US>&lt;/SPAN>&lt;/P>&#xD;
