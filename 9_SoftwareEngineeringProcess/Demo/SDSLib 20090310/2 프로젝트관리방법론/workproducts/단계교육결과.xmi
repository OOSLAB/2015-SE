    name="단계교육결과,_etQhQM0WEd2A0Kjz7fgG4Q" guid="-PaLAAlGGkL5sF1-bezQFmw" changeDate="2009-02-26T15:14:38.656+0900">
  <mainDescription>&lt;P class=S1 style=&quot;MARGIN: 0cm 0cm 0pt 21.25pt&quot;>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>작성항목&lt;/SPAN>&lt;/P>&#xD;
&lt;P class=S1 style=&quot;MARGIN: 0cm 0cm 0pt 21.25pt&quot;>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>&lt;/SPAN>&amp;nbsp;&lt;/P>&#xD;
&lt;P class=S1 style=&quot;MARGIN: 0cm 0cm 0pt 21.25pt&quot;>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>1. &lt;/SPAN>&lt;SPAN style=&quot;FONT-FAMILY: 굴림&quot;>단계교육결과&lt;SPAN lang=EN-US> : &lt;/SPAN>해당 단계별로 실시된 교육정보를 기재한다&lt;SPAN lang=EN-US>.&lt;?xml:namespace prefix = o ns = &quot;urn:schemas-microsoft-com:office:office&quot; />&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/P>&#xD;
&lt;UL>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>단계&lt;SPAN lang=EN-US> : &lt;/SPAN>해당 수행단계를 기록한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>교육명&lt;SPAN lang=EN-US> : &lt;/SPAN>교육명을 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>강사&lt;SPAN lang=EN-US> : &lt;/SPAN>강사명을 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>실시일시&lt;SPAN lang=EN-US> : &lt;/SPAN>실제 교육수행일시&lt;SPAN lang=EN-US>(&lt;/SPAN>시작일시 및 종료일시&lt;SPAN lang=EN-US>)&lt;/SPAN>를 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>대상인원&lt;SPAN lang=EN-US> : &lt;/SPAN>교육대상인원수를 기재한다&lt;SPAN lang=EN-US>. &lt;/SPAN>참석자명단의 인원수와 일치해야 한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>참석인원&lt;SPAN lang=EN-US> : &lt;/SPAN>실제 교육참석인원수를 기재한다&lt;SPAN lang=EN-US>. &lt;/SPAN>참석자명단의 서명자수와 일치해야 한다&lt;SPAN lang=EN-US>.&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&lt;/UL>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>&lt;SPAN lang=EN-US>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;FONT-FAMILY: 굴림&quot;>&#xD;
&lt;P class=S1 style=&quot;MARGIN: 0cm 0cm 0pt 21.25pt&quot;>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>2.&amp;nbsp; &lt;/SPAN>참석자명단&lt;SPAN lang=EN-US> : &lt;/SPAN>각 교육별 참석자 정보를 기재한다&lt;SPAN lang=EN-US>. &lt;/SPAN>참석대상이 되는 교육생의 소속&lt;SPAN lang=EN-US>, &lt;/SPAN>성명&lt;SPAN lang=EN-US>, &lt;/SPAN>직급&lt;SPAN lang=EN-US>, &lt;/SPAN>담당업무를 기재한다&lt;SPAN lang=EN-US>. &lt;/SPAN>&lt;/P>&#xD;
&lt;P class=S1 style=&quot;MARGIN: 0cm 0cm 0pt 21.25pt&quot;>&lt;SPAN lang=EN-US>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>서명은 교육 수행 시 실제 참석자의 경우만 서명하도록 한다&lt;SPAN lang=EN-US>. &lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/P>&#xD;
&lt;UL>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>소속&lt;SPAN lang=EN-US> : &lt;/SPAN>교육생의 소속회사명을 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>성명&lt;SPAN lang=EN-US> : &lt;/SPAN>교육생의 성명을 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>직급&lt;SPAN lang=EN-US> : &lt;/SPAN>교육생의 직급을 기재한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>담당업무&lt;SPAN lang=EN-US> ; &lt;/SPAN>교육생의 프로젝트 내의 해당 업무를 기재한다&lt;SPAN lang=EN-US>. &lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&#xD;
&lt;LI>&#xD;
&lt;DIV class=S3 style=&quot;MARGIN: 0cm 0cm 0pt 66pt; TEXT-INDENT: -20pt; tab-stops: list 66.0pt&quot;>&lt;SPAN lang=EN-US style=&quot;COLOR: windowtext; FONT-FAMILY: Wingdings; mso-bidi-font-family: Wingdings; mso-fareast-font-family: Wingdings&quot;>&lt;SPAN style=&quot;mso-list: Ignore&quot;>&lt;SPAN style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/SPAN>&lt;/SPAN>&lt;/SPAN>&lt;SPAN style=&quot;COLOR: windowtext; FONT-FAMILY: 굴림&quot;>서명&lt;SPAN lang=EN-US> : &lt;/SPAN>교육 수행 시 참석자로 하여금 서명하도록 한다&lt;SPAN lang=EN-US>.&lt;o:p>&lt;/o:p>&lt;/SPAN>&lt;/SPAN>&lt;/DIV>&lt;/LI>&lt;/UL></mainDescription>
  <keyConsiderations>&lt;p>&#xD;
    작성시 고려사항&#xD;
&lt;/p>&#xD;
&lt;ul>&#xD;
    &lt;li>&#xD;
        참석자의 서명을 받은 참석자명단은 바인딩하여 관리하도록 한다.&#xD;
    &lt;/li>&#xD;
&lt;/ul>&lt;br /></keyConsiderations>
  <impactOfNotHaving>&lt;p>&#xD;
    미작성시 영향&#xD;
&lt;/p>&#xD;
&lt;ul>&#xD;
    &lt;li>&#xD;
        착수단계에 수립한 교육계획에 따라 적절하게 교육이 실시되었는지를 파악하기 위해, 교육 수행 시 반드시 교육결과(참석자명단, 교육시간 등)을 기록하도록 한다.&lt;br />&#xD;
    &lt;/li>&#xD;
&lt;/ul></impactOfNotHaving>
  <representationOptions>&lt;p>&#xD;
    작성이 불필요한 경우&#xD;
&lt;/p>&#xD;
&lt;ul>&#xD;
    &lt;li>&#xD;
        해당 단계의 교육계획이 없는 경우는 작성하지 않는다.&#xD;
    &lt;/li>&#xD;
&lt;/ul></representationOptions>
</org.eclipse.epf.uma:ArtifactDescription>
